{
  "name": "@speira/e-commerce-layer-nodejs",
  "version": "1.0.0",
  "description": "Lambda layer for Node.js dependencies",
  "type": "module",
  "exports": {
    ".": "./index.ts",
    "./auth": "./src/auth/index.ts",
    "./database": "./src/database/index.ts",
    "./env": "./src/env.ts",
    "./error": "./src/error/index.ts",
    "./logger": "./src/logger/index.ts",
    "./repositories": "./src/repositories/index.ts",
    "./response": "./src/response.ts",
    "./effectUtils": "./src/effectUtils.ts"
  },
  "main": "./index.ts",
  "scripts": {
    "build": "tsc",
    "build:prod": "tsc --noEmit false && pnpm prune --prod",
    "clean": "rm -rf dist",
    "test": "jest --config=jest.config.js",
    "test:coverage": "jest --config=jest.config.js --coverage",
    "test:watch": "jest --config=jest.config.js --watch"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.890.0",
    "@aws-sdk/lib-dynamodb": "^3.890.0",
    "@effect/schema": "^0.75.5",
    "@speira/e-commerce-lib": "workspace:*",
    "@speira/e-commerce-schema": "workspace:*",
    "effect": "^3.18.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.4",
    "typescript": "^5.0.0"
  }
}
